{
  "name": "realtime-examples",
  "version": "0.1.0",
  "private": true,
  "main": "electron-main.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "next dev --experimental-https --experimental-https-key ./localhost-key.pem --experimental-https-cert ./localhost.pem",
    "build:worklet": "tsc public/worklets/resampling-processor.ts --outDir public/worklets --target es6 --noEmit false --skipLibCheck true",
    "build": "npm run build:worklet && next build",
    "start": "next start",
    "lint": "next lint",
    "build-check": "./scripts/build.sh",
    "electron": "electron .",
    "electron:prod": "npm run build && npm run start & npm run electron",
    "package-mac": "electron-packager . MyAppName --platform=darwin --arch=x64 --out=release-builds --overwrite"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.16",
