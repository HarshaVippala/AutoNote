    "openai": "^4.77.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-draggable": "^4.4.6",
    "react-markdown": "^9.0.3",
    "uuid": "^11.0.4",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.6",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.17.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "dotenv": "^16.5.0",
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
